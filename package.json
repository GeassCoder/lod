{
	"name": "lod",
	"version": "1.5.1",
	"description": "A lightweight library to replace lodash.",
	"main": "src/index.js",
	"scripts": {
		"lint": "eslint \"src/**/*.js\" \"spec/**/*.spec.js\" \"spec/helpers/*.js\"",
		"test": "npm-run-all test:unit test:coverage",
		"test:unit": "istanbul cover -i \"src/**/*.js\" --include-all-sources -x \"src/index.js\" jasmine",
		"test:coverage": "istanbul check-coverage --statements 100 --branches 100",
		"launch-coverage": "open coverage/lcov-report/index.html",
		"doc": "rimraf docs && jsdoc -c jsdoc.json",
		"launch-doc": "npm run doc && open docs/index.html",
		"pre-commit": "npm-run-all --parallel lint test"
	},
	"_moduleAliases": {
		"@src": "src"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@bitbucket.org/jl442741/lod.git"
	},
	"keywords": [
		"lod"
	],
	"pre-commit": [
		"pre-commit"
	],
	"author": "Jason",
	"license": "ISC",
	"homepage": "https://bitbucket.org/jl442741/lod#readme",
	"devDependencies": {
		"docdash": "^1.1.1",
		"eslint": "^5.16.0",
		"istanbul": "^0.4.5",
		"jasmine": "^3.4.0",
		"jsdoc": "^3.6.2",
		"npm-run-all": "^4.1.5",
		"pre-commit": "^1.2.2",
		"rimraf": "^2.6.3"
	},
	"dependencies": {
		"module-alias": "^2.2.0"
	}
}
