{
	"name": "lod",
	"version": "1.1.0",
	"description": "A lightweight version of lodash. Unused stuff and 'ash' are removed for simplicity.",
	"main": "src/index.js",
	"scripts": {
		"lint": "eslint \"src/**/*.js\" \"spec/**/*.spec.js\" \"spec/helpers/*.js\"",
		"test": "npm-run-all test:unit test:coverage",
		"test:unit": "istanbul cover -i src/**/*.js --include-all-sources -x src/index.js jasmine",
		"test:coverage": "istanbul check-coverage --statements 100 --branches 100",
		"pre-commit": "npm-run-all --parallel lint test"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@bitbucket.org/jl442741/lod.git"
	},
	"keywords": [
		"lod"
	],
	"pre-commit": [
		"pre-commit"
	],
	"author": "Jason",
	"license": "ISC",
	"homepage": "https://bitbucket.org/jl442741/lod#readme",
	"devDependencies": {
		"eslint": "^5.16.0",
		"istanbul": "^0.4.5",
		"jasmine": "^3.4.0",
		"npm-run-all": "^4.1.5",
		"pre-commit": "^1.2.2"
	}
}
