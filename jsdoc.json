{
	"tags": {
		"allowUnknownTags": false
	},
	"source": {
		"include": ["./src"],
		"includePattern": ".js$"
	},
	"opts": {
		"encoding": "utf8",
		"template": "node_modules/docdash",
		"readme": "readme.md",
		"destination": "docs/",
		"verbose": true
	},
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": false,
		"default": {
			"outputSourceFiles": false,
			"includeDate": false
		}
	},
	"docdash": {
		"sort": true,
		"search": true
	}
}